{{define "create"}}
{{template "header" .IndexContext }}
{{with $ctx := .}}
{{if .Success}}
<h2>Success!</h2>
{{end}}
<form method="post" action="{{.Reverser.Create $ctx.Collection}}">
{{.Form.ExecuteText}}
</form>

<a class="btn" href="{{.Reverser.List $ctx.Collection}}">Back</a>
{{template "footer"}}
{{end}}
{{end}}